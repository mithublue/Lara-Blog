models:
    Attachment: { post_id: 'foreign:posts', file_name: string, file_path: string, file_type: string, timestamps: timestamps, relationships: { belongsTo: Post } }
    Like: { post_id: 'foreign:posts', user_id: 'foreign:users', timestamps: timestamps, relationships: { belongsTo: 'Post, User' } }
    PostView: { post_id: 'foreign:posts', views_count: 'bigInteger default:0', timestamps: timestamps, relationships: { belongsTo: Post } }
    User: { name: string, email: 'string unique', email_verified_at: 'nullable timestamp', password: string, remember_token: 'nullable string:100', timestamps: timestamps }
    Post: { user_id: 'foreign:users', title: string, slug: 'string unique', body: text, status: 'enum:"draft,published,archived" default:''draft''', published_at: 'nullable timestamp', timestamps: timestamps, relationships: { hasMany: 'Comment, Attachment', belongsToMany: 'Category, Tag' } }
    Category: { name: string, slug: 'string unique', description: 'nullable text', timestamps: timestamps, relationships: { belongsToMany: Post } }
    Tag: { name: string, slug: 'string unique', timestamps: timestamps, relationships: { belongsToMany: Post } }
    Comment: { post_id: 'foreign:posts', user_id: 'foreign:users', body: text, parent_id: 'nullable foreign:comments', timestamps: timestamps, relationships: { belongsTo: 'Post, User' } }
