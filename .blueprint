    - database/migrations/2024_09_09_052130_create_categories_table.php
    - database/migrations/2024_09_09_052131_create_tags_table.php
    - database/migrations/2024_09_09_052132_create_comments_table.php
    - database/migrations/2024_09_09_052133_create_attachments_table.php
